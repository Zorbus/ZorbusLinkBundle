<div class="row-fluid">
    <div class="well">
        <h2>{{ category }}</h2>
        <div class="tab-content">
            <ul>
            {% for link in links %}
                <li><a href="{{ link.url }}" target="{{ link.target }}">{{ link.title | raw }}</a></li>
            {% endfor %}
            </ul>
        </div>
    </div>
</div>
